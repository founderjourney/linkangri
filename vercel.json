{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "outputDirectory": ".next",
  "functions": {
    "src/app/api/**/*.ts": {
      "maxDuration": 10
    }
  },
  "crons": [],
  "env": {
    "NEXT_PUBLIC_APP_NAME": "Digitaliza",
    "NEXT_PUBLIC_SITE_URL": "https://digitaliza.vercel.app"
  },
  "build": {
    "env": {
      "NEXT_PUBLIC_APP_NAME": "Digitaliza"
    }
  },
  "regions": ["iad1"],
  "git": {
    "deploymentEnabled": {
      "main": true,
      "preview": true
    }
  }
}